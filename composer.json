{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "innoboxrr/laravel-setup": "^4.0",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "algolia/scout-extended": "^3.1",
        "google/recaptcha": "^1.3",
        "innoboxrr/aws-file-manager": "^0.0",
        "innoboxrr/laravel-audit": "^1.0",
        "innoboxrr/laravel-auth": "^4.0",
        "innoboxrr/laravel-env-editor": "dev-master",
        "innoboxrr/laravel-notifications": "^1.0",
        "innoboxrr/laravel-uploads": "^1.0",
        "innoboxrr/locale-generator": "^1.0",
        "innoboxrr/routes-to-json": "^1.0",
        "innoboxrr/search-surge": "^2.0",
        "innoboxrr/traits": "^1.0",
        "maatwebsite/excel": "^3.1",
        "opcodesio/log-viewer": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "staudenmeir/belongs-to-through": "^2.1",
        "staudenmeir/eloquent-has-many-deep": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "innoboxrr/larapack-generator": "^5.0",
        "lab404/laravel-impersonate": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/app.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}